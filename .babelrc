{
  "presets": [
    "es2015-no-commonjs",
    "react",
    "stage-2"
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-react-jsx-source",
        "istanbul"
      ]
    },
    "production": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
